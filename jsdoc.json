{
  "opts": {
    "destination": "./docs",
    "nocolor": true,
    "readme": "README.md",
    "recurse": true,
    "template": "node_modules/better-docs",
    "verbose": true
  },
  "plugins": [
    "node_modules/better-docs/category"
  ],
  "source": {
    "include": ["lib"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "tags": {
    "allowUnknownTags": ["category"]
  },
  "templates": {
    "better-docs": {
      "navLinks": [
        {
          "label": "GitHub",
          "href": "https://github.com/pine/safe-storage"
        }
      ]
    }
  }
}
